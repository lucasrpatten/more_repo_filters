{
  "name": "repo-search",
  "version": "0.0.2 ALPHA",
  "description": "A search engine for both beginners and experts to find git repos to contribute to.",
  "main": "web/js/main.jsx",
  "scripts": {
    "build": "npx webpack --config webpack.prod.js || npm run minify",
    "prettyjs": "bash ./automation/pretty-js.sh",
    "dev": "npx webpack serve --open --config webpack.dev.js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d bundle",
    "minify": "uglifyjs ./bundle/* --mangle --compress ",
    "commit": "rm -r ./bundle || bash ./automation/commit.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucasrpatten/repo-search.git"
  },
  "keywords": [
    "Git",
    "GitHub",
    "Search",
    "Git Search",
    "GitHub Search",
    "Search Engine",
    "React"
  ],
  "author": "lucasrpatten",
  "license": "MIT",
  "homepage": "https://lucasrpatten.github.io/repo-search/",
  "bugs": {
    "url": "https://github.com/lucasrpatten/repo-search/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/node": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.6.3",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.2.5",
    "css-loader": "^0.28.11",
    "gh-pages": "^3.2.3",
    "html-webpack-plugin": "^5.5.0",
    "prettier": "2.6.2",
    "raw-loader": "^4.0.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^6.3.0",
    "style-loader": "^3.3.1",
    "uglify-js": "^3.15.4",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0",
    "webpack-merge": "^5.8.0"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "quoteProps": "consistent"
  },
  "lint-staged": {
    "**/*": "prettier --write ."
  },
  "sideEffects": [
    "react",
    "*.css"
  ]
}
